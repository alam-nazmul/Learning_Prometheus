[Unit]
Description=Node Exporter
Wants=network-online.target
After=network-online.target

[Service]
Type=service
PrivateTmp=true
User=node_exporter
Group=node_exporter
Type=simple
ExecStart=/usr/local/bin/node_exporter-1.9.1.linux-amd64/node_exporter
Restart=always
RestartSec=15s


[Install]
WantedBy=multi-user.target